      <%= form_for article do |f|%>
      <div class="form-group">
        <%= f.label :title, "タイトル" %>
        <%= f.text_field :title, class: "btn-square" %>
      </div>
      <div class="form-group">
      <%= f.label :body, "本文" %>
        <%= f.text_area :body, rows: 10%>
      </div>
        <%= f.submit class: "btn-square" %>
      <% end %>